    <rdfs:label xml:lang="it">prezzo</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/ido/includesVAT">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#valueAddedTaxIncluded"/>
    <rdfs:comment xml:lang="it">Indica se il prezzo include o meno l'IVA.</rdfs:comment>
