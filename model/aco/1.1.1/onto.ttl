ido:ArticleCategory a owl:Class ;
    rdfs:label "article category"@en,
        "categoria articolo"@it ;
    rdfs:comment "This class enables the specification of the types of clothes that can be associated to the various styles in the catalogue, such as \"sweater\", \"trousers\", etc. All these types must be organised as a taxonomy."@en,
        "Questa classe permette di specificare i tipi che possono essere associati ai vari modelli del catalago, come ad esempio \"maglia\", \"pantalone\", ecc. Tutti i tipi devono organizzati sotto forma tassonomica."@it ;
    rdfs:isDefinedBy ido:aco ;
    rdfs:subClassOf skos:Concept .

ido:category a owl:NamedIndividual,
        skos:ConceptScheme ;
    rdfs:label "IMPERIAL article category thesaurus"@en,
        "Tesauro IMPERIAL delle categorie degli articoli"@it ;
    skos:hasTopConcept category:blazer,
        category:coat,
        category:dress,
        category:fur-neck,
        category:hat,
        category:heavy-jacket,
        category:jacket,
        category:leather-jacket,
        category:perfume,
        category:shirt,
        category:skirt,
        category:sweater,
        category:tracksuit,
        category:trousers,
        category:vest,
        category:waistcoat .

