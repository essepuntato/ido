        ido:ArticleCategory ;
    rdfs:label "blazer"@en,
        "giacca"@it ;
    skos:inScheme ido:category ;
    skos:notation "J" ;
    skos:prefLabel "blazer"@en,
        "giacca"@it ;
    skos:topConceptOf ido:category .

