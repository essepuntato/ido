ido:aco a owl:Ontology ;
    rdfs:label "ACO" ;
    dc:creator "Silvio Peroni (http://orcid.org/0000-0003-0530-4305)" ;
    dc:date "2017-03-23" ;
    dc:rights "This work is distributed under a [Creative Commons Attribution 4.0 International (CC BY 4.0)](http://creativecommons.org/licenses/by/4.0/)."@en,
        "Questo lavoro Ã¨ distribuito sotto una licenza [Creative Commons Attribution 4.0 International (CC BY 4.0)](http://creativecommons.org/licenses/by/4.0/)."@it ;
    dc:title "Article Category Ontology"@en ;
    rdfs:comment "This ontology defines a taxonomy of all the categories for the articles described by means of the Imperial Data Ontology (IDO, https://w3id.org/ido)."@en,
        "Questa ontologia definisce una tassonomia delle categorie degli articoli descrivibili usando la Imperial Data Ontology (IDO, https://w3id.org/ido)."@it ;
    owl:imports <http://www.w3.org/2004/02/skos/core> ;
    owl:versionIRI <https://w3id.org/ido/aco/1.1.1> ;
    owl:versionInfo "1.1.1" .

category:blazer a owl:NamedIndividual,
