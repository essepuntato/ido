                <owl:onDataRange rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&schema;orderedItem"/>
                <owl:allValuesFrom rdf:resource="http://www.essepuntato.it/2015/07/ido/Piece"/>
