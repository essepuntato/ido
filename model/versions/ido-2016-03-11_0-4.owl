                </owl:onProperty>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&schema;orderedItem"/>
                <owl:someValuesFrom rdf:resource="http://www.essepuntato.it/2015/07/ido/Piece"/>
