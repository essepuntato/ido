    <!-- http://schema.org/containedIn -->

    <owl:ObjectProperty rdf:about="&schema;containedIn">
        <rdfs:label xml:lang="en">is contained in</rdfs:label>
        <rdfs:label xml:lang="it">è contenuto in</rdfs:label>
        <rdfs:comment xml:lang="en">The containement relation between two places, where the first one is contained in the second one.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione di contenimento tra due luoghi, dove il primo è contenuto nel secondo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://schema.org"/>
        <rdfs:domain rdf:resource="&schema;Place"/>
        <rdfs:range rdf:resource="&schema;Place"/>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/example"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/homeLocation -->

    <owl:ObjectProperty rdf:about="&schema;homeLocation">
        <rdfs:label xml:lang="en">has home location</rdfs:label>
        <rdfs:label xml:lang="it">ha indirizzo di residenza</rdfs:label>
        <rdfs:comment xml:lang="en">The link between a person and her home address.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione che lega una persona con il suo indirizzo di residenza.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://schema.org"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/orderedItem -->

    <owl:ObjectProperty rdf:about="&schema;orderedItem">
        <rdfs:label xml:lang="en">has ordered item</rdfs:label>
        <rdfs:label xml:lang="it">ha capo ordinato</rdfs:label>
        <rdfs:comment xml:lang="en">The link between an order and the items which it includes that have been bought by a customer.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione che mette in relazione un ordine con i capi ad esso associati e acquistati da un cliente.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://schema.org"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasArticleCategory -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasArticleCategory">
        <rdfs:label xml:lang="en">has article category</rdfs:label>
        <rdfs:label xml:lang="it">ha categoria articolo</rdfs:label>
        <rdfs:comment xml:lang="en">This property allows one to link a style with its related category.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà permette di collegare un modello con la sua categoria relativa.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:range rdf:resource="http://www.essepuntato.it/2015/07/ido/ArticleCategory"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/Style"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasColour -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasColour">
        <rdfs:label xml:lang="en">has colour</rdfs:label>
        <rdfs:label xml:lang="it">ha colore</rdfs:label>
        <rdfs:comment xml:lang="en">The relationship that allows the link between a stock keeping unit and the associated colour.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un articolo gestito in magazzino con il relativo colore associato.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&gr;qualitativeProductOrServiceProperty"/>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:range rdf:resource="http://www.essepuntato.it/2015/07/ido/Colour"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/StockKeepingUnit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasDeliveryLocation -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasDeliveryLocation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has delivery location</rdfs:label>
        <rdfs:label xml:lang="it">ha indirizzo di spedizione</rdfs:label>
        <rdfs:comment xml:lang="en">The link between an order and the related delivery address.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione che lega un ordine con l&apos;indirizzo a cui deve essere evaso.</rdfs:comment>
        <rdfs:range rdf:resource="&schema;Place"/>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/Order"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasPrice -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasPrice">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">has price</rdfs:label>
        <rdfs:label xml:lang="it">ha prezzo</rdfs:label>
        <rdfs:comment xml:lang="en">It allows one to link an item, stock keeping unit, or piece to the related selling price.</rdfs:comment>
        <rdfs:comment xml:lang="it">Permette di associare un capo, articolo presente in magazzino, o pezzo con il relativo prezzo a cui viene venduto.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:range rdf:resource="http://www.essepuntato.it/2015/07/ido/Price"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.essepuntato.it/2015/07/ido/Item"/>
                    <rdf:Description rdf:about="http://www.essepuntato.it/2015/07/ido/Piece"/>
                    <rdf:Description rdf:about="http://www.essepuntato.it/2015/07/ido/StockKeepingUnit"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasPriceType -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasPriceType">
        <rdfs:label xml:lang="en">has price type</rdfs:label>
        <rdfs:label xml:lang="it">ha tipo di prezzo</rdfs:label>
        <rdfs:comment xml:lang="en">It allows the specification of the type (e.g., net and gross) of a certain price.</rdfs:comment>
        <rdfs:comment xml:lang="it">Permette di specificare il tipo (ad esempio, netto o lordo) di un certo prezzo.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/Price"/>
        <rdfs:range rdf:resource="http://www.essepuntato.it/2015/07/ido/PriceType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasSize -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has size</rdfs:label>
        <rdfs:label xml:lang="it">ha taglia</rdfs:label>
        <rdfs:comment xml:lang="en">The relationship that allows the link between a stock keeping unit and the associated size.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un articolo gestito in magazzino con la relativa taglia associata.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&gr;qualitativeProductOrServiceProperty"/>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:range rdf:resource="http://www.essepuntato.it/2015/07/ido/Size"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/StockKeepingUnit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasTotalPrice -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasTotalPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has total price</rdfs:label>
        <rdfs:label xml:lang="it">ha prezzo totale</rdfs:label>
        <rdfs:comment xml:lang="en">The link between an order and the total price paid by a person for the purchase of that order.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione tra un ordine e il prezzo totale pagato dall&apos;utente associato.</rdfs:comment>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/Order"/>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/hasValidityInterval -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2015/07/ido/hasValidityInterval">
        <rdfs:label xml:lang="en">has validity interval</rdfs:label>
        <rdfs:label xml:lang="it">ha intervallo di validità</rdfs:label>
        <rdfs:comment xml:lang="en">A period in which a particular price is associated to a certain object (e.g., an item).</rdfs:comment>
        <rdfs:comment xml:lang="it">Permette di specificare un periodo temporale in cui un certo prezzo è associato ad un certo oggetto (ad esempio un capo).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2015/07/ido/Price"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ti;TimeInterval"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ti;hasIntervalDate"/>
                        <owl:allValuesFrom rdf:resource="&xsd;dateTime"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2015/07/ido/included -->

