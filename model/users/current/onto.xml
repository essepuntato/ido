    <rdfs:label>IDO-U</rdfs:label>
    <owl:versionIRI rdf:resource="https://w3id.org/ido/users/1.0.1"/>
    <rdfs:comment xml:lang="en">The Imperial Data Ontology (IDO) is an OWL 2 DL ontology that allows the description of selling data of fashion design industries. This module describes the users.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://schema.org/gender">
    <rdfs:comment xml:lang="en">The property that allows one to specify the gender of a person.</rdfs:comment>
