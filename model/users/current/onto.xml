    <rdfs:label xml:lang="it">ha data di nascita</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://schema.org"/>
    <rdfs:comment xml:lang="en">The property that allows one to specify the birth date of a person.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
