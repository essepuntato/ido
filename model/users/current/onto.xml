    <rdfs:domain rdf:resource="http://schema.org/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/ido/wasMadeBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
