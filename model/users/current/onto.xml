    <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/users"/>
    <rdfs:subClassOf rdf:nodeID="Ne8e443734bc3487198423d1890692461"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://schema.org/homeLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
