        <owl:inverseOf rdf:resource="https://w3id.org/ido/wasMadeBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/UserAccount"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Order"/>
        <rdfs:comment xml:lang="it">La relazione che lega un utente all&apos;ordine da esso effettuato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a user and the order it has done.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/users"/>
        <rdfs:label xml:lang="it">ha effettuato un ordine</rdfs:label>
        <rdfs:label xml:lang="en">made an order</rdfs:label>
    </owl:ObjectProperty>
    


