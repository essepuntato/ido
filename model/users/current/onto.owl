    <!-- https://w3id.org/ido/UserAccount -->

    <owl:Class rdf:about="https://w3id.org/ido/UserAccount">
        <rdfs:subClassOf rdf:resource="http://rdfs.org/sioc/ns#UserAccount"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://xmlns.com/foaf/0.1/account"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/identifier"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">La classe di degli avatar con cui le persone sono riconosciute nella piattaforma di vendita.</rdfs:comment>
        <rdfs:comment xml:lang="en">The class of all the avatars that identify real people in the selling platform.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/users"/>
        <rdfs:label xml:lang="en">user account</rdfs:label>
        <rdfs:label xml:lang="it">utente</rdfs:label>
    </owl:Class>
