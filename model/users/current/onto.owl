    <owl:ObjectProperty rdf:about="https://w3id.org/ido/hasDeliveryLocation">
        <owl:inverseOf rdf:resource="https://w3id.org/ido/isDeliveryLocationIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/Order"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <rdfs:comment xml:lang="it">La relazione che lega un ordine con l&apos;indirizzo a cui deve essere evaso.</rdfs:comment>
        <rdfs:comment xml:lang="en">The link between an order and the related delivery address.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/users"/>
        <rdfs:label xml:lang="it">ha indirizzo di spedizione</rdfs:label>
        <rdfs:label xml:lang="en">has delivery location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isDeliveryLocationIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isDeliveryLocationIn">
        <rdfs:comment xml:lang="it">La relazione tra un certo luogo e l&apos;ordine in cui compare come indirizzo di consegna.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation between a place and the order where it is specified as delivery address.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/users"/>
        <rdfs:label xml:lang="en">is delivery location in</rdfs:label>
        <rdfs:label xml:lang="it">è l&apos;indirizzo di consegna nel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/madeOrder -->

