        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/users"/>
        <rdfs:label xml:lang="en">province</rdfs:label>
        <rdfs:label xml:lang="it">provincia</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ido/Region -->

    <owl:Class rdf:about="https://w3id.org/ido/Region">
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
