    <!-- http://purl.org/dc/terms/identifier -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">An attribute used for associating an identifier to an entity (such as an item).</rdfs:comment>
        <rdfs:comment xml:lang="it">Un attributo usato per associare un identificativo ad un&apos;entità come un capo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms"/>
        <rdfs:label xml:lang="it">ha identificativo</rdfs:label>
        <rdfs:label xml:lang="en">has identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ido/wasDesignedFor -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/ido/wasDesignedFor">
        <rdfs:domain rdf:resource="https://w3id.org/ido/Style"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>female</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>male</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="it">La proprietà che permette di specificare il genere delle persone per le quali un articolo è stato creato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that allows one to specify the gender of the people for whom an article has been created.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was designed for</rdfs:label>
        <rdfs:label xml:lang="it">è stato creato per</rdfs:label>
    </owl:DatatypeProperty>
    


