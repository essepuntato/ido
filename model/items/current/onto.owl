    <!-- https://w3id.org/ido/hasArticleCategory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/hasArticleCategory">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/additionalType"/>
        <rdfs:subPropertyOf rdf:resource="http://schema.org/category"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ido/isArticleCategoryOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/Style"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/ArticleCategory"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di collegare un modello con la sua categoria relativa.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property allows one to link a style with its related category.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="it">ha categoria articolo</rdfs:label>
        <rdfs:label xml:lang="en">has article category</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/hasColour -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/hasColour">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#qualitativeProductOrServiceProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ido/isColourIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/StockKeepingUnit"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Colour"/>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un articolo gestito in magazzino con il relativo colore associato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship that allows the link between a stock keeping unit and the associated colour.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="it">ha colore</rdfs:label>
        <rdfs:label xml:lang="en">has colour</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/hasPrice -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/hasPrice">
        <owl:inverseOf rdf:resource="https://w3id.org/ido/isPriceOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ido/Item"/>
                    <rdf:Description rdf:about="https://w3id.org/ido/Piece"/>
                    <rdf:Description rdf:about="https://w3id.org/ido/StockKeepingUnit"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/ido/Price"/>
        <rdfs:comment xml:lang="en">It allows one to link an item, stock keeping unit, or piece to the related selling price.</rdfs:comment>
        <rdfs:comment xml:lang="it">Permette di associare un capo, articolo presente in magazzino, o pezzo con il relativo prezzo a cui viene venduto.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/items"/>
        <rdfs:label xml:lang="it">ha prezzo</rdfs:label>
        <rdfs:label xml:lang="en">has price</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/hasSize -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/hasSize">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#qualitativeProductOrServiceProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ido/isSizeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/StockKeepingUnit"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Size"/>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un articolo gestito in magazzino con la relativa taglia associata.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship that allows the link between a stock keeping unit and the associated size.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="it">ha taglia</rdfs:label>
        <rdfs:label xml:lang="en">has size</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/included -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/included">
        <owl:inverseOf rdf:resource="https://w3id.org/ido/wasProposedFor"/>
        <rdfs:comment xml:lang="it">La relazione che collega una stagione con tutti i capi proposti per quella stagione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links a season with all the items proposed for such season.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="it">ha incluso</rdfs:label>
        <rdfs:label xml:lang="en">included</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isArticleCategoryOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isArticleCategoryOf">
        <rdfs:comment xml:lang="it">Questa proprietà permette di collegare una categoria di vestiario con i modelli relativi.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property allows one to link an article category with the related articles.</rdfs:comment>
        <rdfs:label xml:lang="en">is article category of</rdfs:label>
        <rdfs:label xml:lang="it">è categoria dell&apos;articolo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isAvailableAs -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isAvailableAs">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#embodiment"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ido/isStockKeepingUnitOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/Item"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/StockKeepingUnit"/>
        <rdfs:comment xml:lang="it">La relazione che associa un certo articolo con la descrizione del suo colore e la sua misura.</rdfs:comment>
        <rdfs:comment xml:lang="en">The link between a certain article and the description of its colour and size.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">is available as</rdfs:label>
        <rdfs:label xml:lang="it">è disponibile in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isColourIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isColourIn">
        <rdfs:comment xml:lang="it">La relazione tra un certo colore e i vari articoli gestiti in magazzino in cui è stato usato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The link between a colour and the stock keeping units that have used it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">is colour in</rdfs:label>
        <rdfs:label xml:lang="it">è un colore di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isPriceOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isPriceOf">
        <rdfs:comment xml:lang="it">It allows to reach the article to which the current price has been applied.</rdfs:comment>
        <rdfs:comment xml:lang="it">Permette di risalire all&apos;articolo a cui il prezzo in considerazione è stato applicato.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ido/items"/>
        <rdfs:label xml:lang="en">is price of</rdfs:label>
        <rdfs:label xml:lang="it">è prezzo di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isProductOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isProductOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#exemplarOf"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ido/wasProducedAs"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="it">La relazione che associa uno specifico capo fisico alla sua relativa materializzazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The link between a concrete physical item and its materialisation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">is a product of</rdfs:label>
        <rdfs:label xml:lang="it">è un prodotto di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isSizeOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isSizeOf">
        <rdfs:comment xml:lang="it">La relazione che permette di collegare la taglia di un articolo it suo relativo articolo gestito in magazzino.</rdfs:comment>
        <rdfs:comment xml:lang="en">The link between a size of an article with its related materialisation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">is size of</rdfs:label>
        <rdfs:label xml:lang="it">è taglia di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/isStockKeepingUnitOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/isStockKeepingUnitOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#hasMakeAndModel"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#embodimentOf"/>
        <rdfs:comment xml:lang="it">La relazione che associa una certa materializzazione di articolo con l&apos;articolo stesso.</rdfs:comment>
        <rdfs:comment xml:lang="it">The link between a certain materialization on an article and the article itself.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">is stock keeping unit of</rdfs:label>
        <rdfs:label xml:lang="it">è articolo gestito in magazzino di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/wasMadeOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/wasMadeOf">
        <owl:inverseOf rdf:resource="https://w3id.org/ido/wasUsedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/Item"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Textile"/>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un capo con il tessuto con cui è stato realizzato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that allows one to link an item with the texture it uses.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was realised with</rdfs:label>
        <rdfs:label xml:lang="it">è stato realizzato con</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/wasProducedAs -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/wasProducedAs">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#exemplar"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/StockKeepingUnit"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Piece"/>
        <rdfs:comment xml:lang="it">La relazione che associa una certa materializzazione di un articolo con uno specifico capo fisico di quell&apos;articolo prodotto.</rdfs:comment>
        <rdfs:comment xml:lang="en">The link between a certain materialisation of an article and one of the concrete physical items of such article.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was produced as</rdfs:label>
        <rdfs:label xml:lang="it">è stato prodotto come</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/wasProposedFor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/wasProposedFor">
        <rdfs:domain rdf:resource="https://w3id.org/ido/Item"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Season"/>
        <rdfs:comment xml:lang="it">La relazione che permette di specificare la stagione a cui un capo è destinato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that allows one to specify the season for which an item was proposed for.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was proposed for</rdfs:label>
        <rdfs:label xml:lang="it">è stato proposto per</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/wasRealisedAccordingTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/wasRealisedAccordingTo">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/goodrelations/v1#hasMakeAndModel"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#realizationOf"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ido/wasRealisedAs"/>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un capo con il suo modello di riferimento.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links an item with its style.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was realised according to</rdfs:label>
        <rdfs:label xml:lang="it">è stato realizzato conformemente a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/wasRealisedAs -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/wasRealisedAs">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#realization"/>
        <rdfs:domain rdf:resource="https://w3id.org/ido/Style"/>
        <rdfs:range rdf:resource="https://w3id.org/ido/Item"/>
        <rdfs:comment xml:lang="it">La relazione che permette di collegare un modello con i capi che sono stati realizzati a partire da quello.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that allows one to link the style with the items that have been realised according to it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was realised as</rdfs:label>
        <rdfs:label xml:lang="it">è stato realizzato come</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ido/wasUsedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ido/wasUsedIn">
        <rdfs:comment xml:lang="it">La relazione che collega un tessuto con tutti i capi che lo hanno usato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links a textile with all the items that have used it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.essepuntato.it/2015/07/ido"/>
        <rdfs:label xml:lang="en">was used in</rdfs:label>
        <rdfs:label xml:lang="it">è stato usato in</rdfs:label>
    </owl:ObjectProperty>
    


